{
  "name": "modullo/modules-auth",
  "description": "Modullo Authentication Module",
  "type": "library",
  "license": "GPL-3.0-or-later",
  "authors": [
    {
      "name": "Ifeoluwa Olawoye",
      "email": "bolaji.olawoye@hostvillenigeria.org"
    },
    {
      "name": "Ayotomide Aina",
      "email": "tomide@hostville.website"
    }
  ],
  "require": {
    "php": ">=7.3",
    "aws/aws-sdk-php": "^3.74",
    "laravel/framework": "^8.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.5"
  },
  "config": {
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Modullo\\ModulesAuth\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Modullo\\ModulesAuth\\ModulesAuthServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "scripts": {


    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan optimize",
      "php artisan vendor:publish"
    ],
    "post-update-cmd": [
      "php artisan optimize",
      "php artisan vendor:publish"
    ]
  },
  "repositories": {
    "private-packagist": {
      "type": "composer",
      "url": "https://repo.packagist.com/modullo/"
    }
  }
}
